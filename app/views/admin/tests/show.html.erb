<p><%= link_to t('.create') ,  new_admin_test_path(@test) %></p>
<%= link_to  t('.edit') , edit_admin_test_path(@test)  %>
<p><strong><%= t('label.title') %></strong>: <%= @test.title %></p>
<p><strong><%= t('label.level') %></strong>: <%= @test.level %></p>
<p><strong><%= t('label.timer') %></strong>: <%= @test.timer %></p>
<p><%= link_to t('.new_question'),  new_admin_test_question_path(@test) %></p>
<table>
  <tr>
    <th>  <%= t('label.question') %></th>
    <th colspan="3"><%= t('label.actions') %></th>
  </tr>
  <ol>
    <%= render @test.questions %>
  </ol>
</table>
<p><%= link_to t('actions.back'), admin_tests_path  %></p>
