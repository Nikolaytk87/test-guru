<p>Create new Question for Test number <%= params[:test_id] %></p>
<form action="<%= test_questions_path %>" accept-charset="UTF-8" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>
    <label for="question-body">Body</label><br>
    <input type="text" name="question[body]" id="question-body">
  </p>
  <br>
  <div>
    <input type="submit" name="commit" value="Create Question">
  </div>
</form>
